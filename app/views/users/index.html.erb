<p id=”alert”><%= alert %></p>
<% if current_user.present? %>
  <%= render :partial => "user_square_grid"%>  
<% else %>

  <%= form_tag sessions_path do |form| %>
    <div class="form-group">
      <div class="btn btn-secondary center">
        <%= submit_tag "Start Session", :class => "btn btn-primary"%>
      </div>
    </div>
  <% end %>
<% end %>
